{
  "client": {
    "name": "<%= clientName %>",
    "logo": false,
    "homepage": "<%= clientHomepage %>",
    "webfonts": {
      "typekit": false,
      "googleFonts": false
    }
  },
  "author": {
    "name": "<%= authorName %>",
    "email": "<%= authorEmail %>"
  },
  "sections": {
    "Style Tile": "index.html",
    "Elements": "sections/markup/elements.html",
    "Typography": "sections/typography/fillerama.html",
    "UI Patterns": "sections/ui/patterns.html",
    "Color Guide": "colors.html"
  },
  "styleTile": {
    "colors": 5,
    "images": [
      "images/moon.jpg",
      "images/ice.jpg",
      "images/iceberg.jpg",
      "images/clouds.jpg",
      "http://lorempixel.com/output/abstract-q-c-640-480-1.jpg"
    ],
    "keywords": [
      "Default",
      "Minimal",
      "Nothing",
      "Design me!"
    ],
    "buttons": {
      "Call to Action": "call-to-action",
      "Big Button": "big-button",
      "Warning Button": [
        "big-button",
        "red"
      ]
    }
  },
  "colors": {
    "variableNames": [
      "black",
      "primary",
      "secondary",
      "tertiary",
      "quadrary"
    ],
    "shades": 6
  },
  "server": {
    "port": 8000
  },
  "export": {
    "path": "export",
    "assetPrefix": "<%= clientSlug %>"
  },
  "compass": {
    "debugInfo": false,
    "extensions": [
      "style-prototypes"
    ]
  },
  "git": {
    "defaultCommit": "Production build and commit"<% if (ghDeploy) {%>,
    "deployUpstream": "origin",
    "deployBranch": "gh-pages"<% } %>
  }
}